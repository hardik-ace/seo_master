<div class="row">
    <!-- Link Type Table Start -->
    <div class="col-md-12">
        <div class="crawler-dashboard-section crawler-dashboard-main">
            <!-- Table responsive Start -->
            <div class="card">
                <div class="seo-checker-data-table">
                    <div class="table-section">
                        <div class="site-audit-table">
                            <div class="table-responsive">
                                <table class="table table-borderless table-striped mb-0">
                                    <thead>
                                        <tr>
                                            <th scope="col">Link Type</th>
                                            <th scope="col">Backlinks</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>Crawled Links:</td>
                                            <td>
                                                <%= crawledLinks.length %>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>Broken Links:</td>
                                            <td>
                                                <%= brokenLinks.length %>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>Links to Crawl:</td>
                                            <td>
                                                <%= toCrawl.length %>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Table responsive Start -->
        </div>
    </div>
    <!-- Link Type Table End -->
    <!-- Crawled Links Table Start -->
    <div class="col-md-12">
        <h3 class="second-table">Crawled Links</h3>
        <div class="card">
        <div class="second-table-section">
            <div class="seo-checker-data-table seo-checker-table crawled-links-table">
                <div class="table-section">
                    <div class="site-audit-table">
                        <div class="table-responsive">
                            <table class="table table-borderless table-striped defaultDataTable">
                                <thead>
                                    <tr>
                                        <th scope="col">Links</th>
                                        <th scope="col">Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% crawledLinks.forEach(function(crawledLink) { %>
                                        <tr>
                                            <td><a href="<%= crawledLink %>" target="_blank">
                                                    <%= crawledLink %>
                                                </a></td>
                                                <td>
                                                    <a href="/broken/findBrokenLinks?id=<%= id %>&url=<%= encodeURIComponent(crawledLink); %>" target="_blank" class="btn btn-primary audited-btn">Generate Seo Report</a>
                                                </td>
                                        </tr>
                                        <% }); %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        </div>
    </div>
    <!-- Crawled Links Table End -->


    <!-- Broken Links Table Start -->
    <div class="col-md-12">
        <h3 class="second-table">Broken Links</h3>
        <div class="card">
        <div class="second-table-section">
            <div class="seo-checker-data-table seo-checker-table">
                <div class="table-section">
                    <div class="site-audit-table">
                        <div class="table-responsive">
                            <table class="table table-borderless table-striped defaultDataTable">
                                <thead>
                                    <tr>
                                        <th scope="col">Links</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% brokenLinks.forEach(function(brokenLink) { %>
                                        <tr>
                                            <td><a href="<%= brokenLink %>" target="_blank">
                                                    <%= brokenLink %>
                                                </a></td>
                                        </tr>
                                        <% }); %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Broken Links Table End -->

</div>







<script>
    $( document ).ready(function() {
        let table = new DataTable('.defaultDataTable', {
            pageLength: 10,
            responsive: true
        });
    });
</script>